<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EspinhoAI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EspinhoAI"
    Shell.FlyoutBehavior="Flyout"
    Title="espinhoai">

    <ShellContent
        Title="DoHomecs"
        ContentTemplate="{DataTemplate local:HomePage}"
        Route="home" />
        
     <ShellContent
        Title="Docs"
        ContentTemplate="{DataTemplate local:DocsPage}"
        Route="docs" />

     <ShellContent
        Title="Extract"
        ContentTemplate="{DataTemplate local:ExtractPage}"
        Route="extract" />

    <ShellContent
        Title="main"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="main" />

     <ShellContent
        Title="INferance"
        ContentTemplate="{DataTemplate local:InferancePage}"
        Route="inferance" />


</Shell>
